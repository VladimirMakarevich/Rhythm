  <elmah>
    <!--<errorMail from="elmah@dogcoding.com"
             to="justadreampictures@gmail.com"
             subject="Exception in occured in DogCoding"
             priority="High"
             async="true"
             smptserver="smpt.gmail.com"
             useSsl="false"
             noYsod="true" />-->
    <errorFilter>
          <test>
                <jscript>
                      <expression>
                            <![CDATA[
                // @assembly mscorlib
                // @assembly System.Web, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a
                // @import System.IO
                // @import System.Web
 
                HttpStatusCode == 404
                || BaseException instanceof FileNotFoundException
                || BaseException instanceof HttpRequestValidationException
                /* Using RegExp below (see http://msdn.microsoft.com/en-us/library/h6e2eb7w.aspx) */
                || Context.Request.UserAgent.match(/crawler/i)
                || Context.Request.ServerVariables['REMOTE_ADDR'] == '127.0.0.1' // IPv4 only
                ]]>                 
          </expression>       
        </jscript>      
      </test>
    </errorFilter>
    <security allowRemoteAccess="true" />
    <errorLog type="Elmah.SqlErrorLog, Elmah" connectionStringName="DogCodingContext" />
  </elmah>
  <location path="elmah.axd" inheritInChildApplications="false">
